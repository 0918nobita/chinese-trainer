{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsxdev",
    "module": "esnext",
    "moduleResolution": "bundler",
    "sourceMap": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
